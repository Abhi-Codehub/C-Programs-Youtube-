// Program: Write a program to print fibonnaci series
#include <stdio.h>

int main()
{
    int n;
    int a = 0;
    int b = 1;
    int c;

    printf("Enter how many terms to print: ");
    scanf("%d", &n);

    printf("fibonnaci Series:\n");
    for(int i = 1; i <= n; i++)
    {
        printf("%d ", a);
        c = a + b;
        a = b;
        b = c;
    }
    return 0;
}
