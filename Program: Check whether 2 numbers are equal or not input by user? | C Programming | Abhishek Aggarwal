// Write a program that checks whether the two numbers entered by the user are equal or not.
#include <stdio.h>

int main()
{
    int num1, num2;
    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);

    // check whether these numbers are equal or not
    if(num1 == num2)
    {
        printf("The given numbers are equal");
    }
    else {
        printf("The given numbers are not equal");
    }

    return 0;
}
