// Program: Write a program to swap the values of two variables with and without using the third variable.

// Swapping using third variable
#include <stdio.h>

int main()
{
    int a, b, temp;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    temp = a;
    a = b;
    b = temp;

    printf("After swapping using third variable: a = %d, b = %d", a, b);

    return 0;
}

// swapping of two numbers without using the third variable
#include <stdio.h>

int main()
{
    int a, b;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    a = a + b;
    b = a - b;
    a = a - b;

    printf("After swapping without using third variable: a = %d, b = %d", a, b);

    return 0;
}
